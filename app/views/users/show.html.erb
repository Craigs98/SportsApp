<div class="container col-md-10 offset-md-1 col-lg-8 offset-lg-2 text-xs-left">
  <aside class="col-md-4">
    <div>
      <h1>My Profile</h1>
    </div>
    <div class="hero-unit user_info">
      <h1><%= @user.email %></h1>
    </div>
  </aside>
</div>



<% @user_posts.each do |post| %>
<div class="container ">
  <label for="colFormLabelSm" class="col-sm-2 col-form-label col-form-label-sm">Title</label>
<%= post.title %>
<%= link_to 'post', post %>
</div>
<% end %>
